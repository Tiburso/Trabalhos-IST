Fila = ['#', A, B, C, D, E, '#', F, '#', G, H, I],  bagof(Espaco, espaco_fila(Fila, Espaco), Res), Res == [[A, B, C, D, E], [G, H, I]], writeln('true'); writeln('false').
Fila = ['#', A, B, C, D, E, '#', F, '#', G, H, I],  espaco_fila(Fila, [A, B, C, D]), writeln('true'); writeln('false').
Fila = ['#', A, B, C, D, E, '#', F, '#', G, H, I],  espaco_fila(Fila, [A, B, C, D, E, '#']), writeln('true'); writeln('false').
